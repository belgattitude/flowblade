{
  "name": "@flowblade/sqlduck",
  "version": "0.0.3",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "license": "MIT",
  "homepage": "https://github.com/belgattitude/flowblade",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/flowblade.git",
    "directory": "packages/sqlduck"
  },
  "scripts": {
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "build": "tsdown",
    "build-release": "yarn build && rimraf ./_release && yarn pack && mkdir ./_release && tar zxvf ./package.tgz --directory ./_release && rm ./package.tgz",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest --ui",
    "test-e2e": "vitest -c vitest.e2e.config.ts run",
    "test-e2e-watch": "vitest -c vitest.e2e.config.ts --ui",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../../.cache/eslint/sqlduck.eslintcache",
    "fix-staged": "lint-staged --allow-empty",
    "check-dist": "es-check --config=.escheckrc.json",
    "check-pub": "publint",
    "check-size": "size-limit"
  },
  "dependencies": {
    "@flowblade/core": "workspace:^",
    "@flowblade/source-duckdb": "workspace:^",
    "@flowblade/sql-tag": "workspace:^",
    "@standard-schema/spec": "^1.1.0",
    "p-mutex": "^1.0.0",
    "valibot": "^1.2.0",
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@duckdb/node-api": "^1.4.3-r.1"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "8.8.0",
    "@dotenvx/dotenvx": "1.51.2",
    "@duckdb/node-api": "1.4.3-r.1",
    "@faker-js/faker": "10.1.0",
    "@flowblade/source-kysely": "workspace:^",
    "@httpx/assert": "0.16.7",
    "@size-limit/esbuild": "12.0.0",
    "@size-limit/file": "12.0.0",
    "@testcontainers/mssqlserver": "11.11.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@traversable/zod": "0.0.57",
    "@types/node": "25.0.3",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "ansis": "4.2.0",
    "browserslist-to-esbuild": "2.1.1",
    "core-js": "3.47.0",
    "cross-env": "10.1.0",
    "es-check": "9.5.3",
    "esbuild": "0.27.2",
    "eslint": "8.57.1",
    "execa": "9.6.1",
    "is-in-ci": "2.0.0",
    "kysely": "0.28.9",
    "npm-run-all2": "8.0.4",
    "prettier": "3.7.4",
    "publint": "0.3.16",
    "regexp.escape": "2.0.1",
    "rimraf": "6.1.2",
    "size-limit": "12.0.0",
    "sql-formatter": "15.6.12",
    "tarn": "3.0.2",
    "tedious": "19.2.0",
    "testcontainers": "11.11.0",
    "tsdown": "0.18.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "directory": "_release/package"
  }
}
