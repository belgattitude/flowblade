{
  "openapi": "3.1.0",
  "info": {
    "title": "Hono Documentation",
    "description": "Development documentation",
    "version": "0.0.0"
  },
  "paths": {
    "/api/demo/duckdb/search": {
      "get": {
        "operationId": "getApiDemoDuckdbSearch",
        "description": "Search",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "meta": {
                      "type": "object",
                      "properties": {
                        "count": {
                          "type": "number"
                        }
                      },
                      "required": ["count"]
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "productId": {
                            "type": "number"
                          },
                          "name": {
                            "type": "string"
                          },
                          "createdAt": {
                            "type": "string"
                          }
                        },
                        "required": ["productId", "name", "createdAt"]
                      }
                    },
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": ["meta", "data"]
                }
              }
            }
          }
        },
        "parameters": [
          {
            "in": "query",
            "name": "min",
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "name",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "createdAt",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "number"
            }
          }
        ]
      }
    },
    "/api/product/ethical/search": {
      "get": {
        "operationId": "getApiProductEthicalSearch",
        "description": "Search for ethical products",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "label": {
                        "type": "string",
                        "description": "The name of the product"
                      },
                      "brand": {
                        "type": "string",
                        "description": "The brand of the product"
                      },
                      "price": {
                        "type": "number"
                      },
                      "stock": {
                        "type": "number"
                      },
                      "weight": {
                        "type": "string"
                      },
                      "color": {
                        "type": "string"
                      },
                      "category": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "label",
                      "brand",
                      "price",
                      "stock",
                      "weight",
                      "color",
                      "category"
                    ]
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "in": "query",
            "name": "brands",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "minPrice",
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "slowdownApiMs",
            "schema": {
              "type": "integer"
            },
            "description": "Artificially slow down the API by this many milliseconds."
          }
        ]
      }
    },
    "/api/product/ethical/brands": {
      "get": {
        "operationId": "getApiProductEthicalBrands",
        "description": "Get list of ethical brands",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      }
                    },
                    "required": ["name"]
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {}
}
