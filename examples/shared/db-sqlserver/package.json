{
  "name": "@examples/db-sqlserver",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "private": true,
  "exports": {
    "./prisma": {
      "types": "./dist/prisma.d.ts",
      "default": "./dist/prisma.js"
    },
    "./kysely-types": {
      "types": "./dist/kysely-types.d.ts",
      "default": "./dist/kysely-types.js"
    },
    "./package.json": "./package.json"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "license": "MIT",
  "homepage": "https://github.com/belgattitude/flowblade",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/flowblade.git",
    "directory": "examples/shared/db-sqlserver"
  },
  "scripts": {
    "clean": "rimraf ./.next ./coverage ./tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest --ui",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../../../.cache/eslint/db-sqlserver.eslintcache",
    "fix-staged": "lint-staged --allow-empty",
    "codegen": "run-s codegen-prisma codegen-ddl",
    "codegen-prisma": "prisma generate",
    "codegen-ddl": "tsx ./scripts/create-ddl.ts",
    "db-recreate": "run db-drop && tsx ./scripts/db-recreate.ts && prisma generate",
    "db-drop": "dotenvx run -- yarn prisma db push --skip-generate --schema=./prisma/schema.reset.prisma --force-reset",
    "prisma-format": "prisma format",
    "prisma-validate": "prisma validate",
    "prisma-generate": "prisma generate",
    "prisma-db-reset-push": "dotenvx run -- yarn prisma db push --force-reset && run codegen-ddl",
    "prisma-db-push": "dotenvx run -- yarn prisma db push",
    "prisma-db-pull": "dotenvx run -- yarn prisma db pull",
    "prisma-db-seed": "dotenvx run -- yarn prisma db seed",
    "prisma-migrate": "dotenvx run -- yarn prisma migrate",
    "prisma-migrate-reset": "dotenvx run -- yarn prisma migrate reset",
    "prisma-studio": "dotenvx run -- yarn prisma studio"
  },
  "peerDependencies": {
    "@prisma/client": "^6.6.0",
    "kysely": "^0.28.0",
    "tedious": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "7.8.0",
    "@dotenvx/dotenvx": "1.49.0",
    "@faker-js/faker": "10.0.0",
    "@flowblade/core": "workspace:^",
    "@flowblade/sql-tag": "workspace:^",
    "@httpx/assert": "0.16.2",
    "@prisma/client": "6.16.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "24.5.0",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "cross-env": "10.0.0",
    "eslint": "8.57.1",
    "execa": "9.6.0",
    "kysely": "0.28.7",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "prisma": "6.16.1",
    "prisma-kysely": "2.2.0",
    "prisma-markdown": "3.0.1",
    "regexp.escape": "2.0.1",
    "rimraf": "6.0.1",
    "tedious": "19.0.0",
    "tinyrainbow": "3.0.3",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "prisma": {
    "seed": "tsx ./scripts/run-prisma-seeds.ts"
  }
}
