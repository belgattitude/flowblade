{
  "name": "@examples/base-ui",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./globals.css": "./src/styles/globals.css",
    "./postcss.config.mjs": "./postcss.config.mjs",
    "./lib/*": "./src/lib/*.ts",
    "./components/*": "./src/components/*.tsx",
    "./hooks/*": "./src/hooks/*.ts"
  },
  "scripts": {
    "clean": "rimraf ./.next ./coverage ./tsconfig.tsbuildinfo",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest --ui",
    "bump-ui": "npx shadcn@latest add --all --overwrite -c . && npx shadcn@latest migrate radix && yarn lint --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../../../.cache/eslint/base-ui.eslintcache",
    "fix-staged": "lint-staged --allow-empty",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tooltip": "1.2.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "lucide-react": "0.546.0",
    "next-themes": "0.4.6",
    "radix-ui": "1.4.3",
    "react": "19.2.0",
    "react-day-picker": "9.11.1",
    "react-dom": "19.2.0",
    "react-hook-form": "7.65.0",
    "react-resizable-panels": "3.0.6",
    "recharts": "2.15.4",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "vaul": "1.1.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "8.3.1",
    "@storybook/addon-docs": "9.1.13",
    "@storybook/addon-vitest": "9.1.13",
    "@storybook/react-vite": "9.1.13",
    "@tailwindcss/postcss": "4.1.16",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitejs/plugin-react-swc": "4.2.0",
    "@vitest/browser": "4.0.3",
    "@vitest/coverage-v8": "4.0.3",
    "@vitest/ui": "4.0.3",
    "eslint": "8.57.1",
    "happy-dom": "20.0.8",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "storybook": "9.1.13",
    "tailwindcss": "4.1.16",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.3"
  }
}
