{
  "name": "@examples/base-ui",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./globals.css": "./src/styles/globals.css",
    "./postcss.config.mjs": "./postcss.config.mjs",
    "./lib/*": "./src/lib/*.ts",
    "./components/*": "./src/components/*.tsx",
    "./hooks/*": "./src/hooks/*.ts"
  },
  "scripts": {
    "clean": "rimraf ./.next ./coverage ./tsconfig.tsbuildinfo",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest --ui",
    "bump-ui": "npx shadcn@latest add --all --overwrite -c . && npx shadcn@latest migrate radix && yarn lint --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../../.cache/eslint/base-ui.eslintcache",
    "fix-staged": "lint-staged --allow-empty",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "lucide-react": "0.561.0",
    "next-themes": "0.4.6",
    "radix-ui": "1.4.3",
    "react": "19.2.3",
    "react-day-picker": "9.12.0",
    "react-dom": "19.2.3",
    "react-hook-form": "7.68.0",
    "react-resizable-panels": "3.0.6",
    "recharts": "2.15.4",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "vaul": "1.1.2",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "8.8.0",
    "@storybook/addon-docs": "10.1.9",
    "@storybook/addon-vitest": "10.1.9",
    "@storybook/react-vite": "10.1.9",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "25.0.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "@vitejs/plugin-react-swc": "4.2.2",
    "@vitest/browser": "4.0.15",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/ui": "4.0.15",
    "eslint": "8.57.1",
    "happy-dom": "20.0.11",
    "playwright": "1.57.0",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "storybook": "10.1.9",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  }
}
