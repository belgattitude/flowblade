{
  "name": "@docs/website",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "license": "MIT",
  "homepage": "https://github.com/belgattitude/flowblade",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/flowblade.git",
    "directory": "docs"
  },
  "scripts": {
    "dev": "next dev --turbopack --port 3008",
    "build": "next build && run generate-search-idx",
    "build-static": "cross-env NEXT_BUILD_OUTPUT=export next build && run generate-search-idx-static",
    "generate-search-idx": "pagefind --site .next/server/app --output-path public/_pagefind",
    "generate-search-idx-static": "pagefind --site .next/server/app --output-path out/_pagefind",
    "bundle-analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "serve": "sirv out --port 3009",
    "clean": "rimraf ./.next ./out ./public/_pagefind ./coverage ./tsconfig.tsbuildinfo",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../.cache/eslint/docs.eslintcache",
    "fix-staged": "lint-staged --allow-empty"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "0.13.8",
    "clsx": "2.1.1",
    "next": "15.4.5",
    "nextra": "5.0.0-alpha.24",
    "nextra-theme-docs": "5.0.0-alpha.24",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "tailwind-merge": "3.3.1",
    "zod": "4.0.13"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "7.5.0",
    "@tailwindcss/postcss": "4.1.11",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "24.1.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "cross-env": "10.0.0",
    "dotenv-expand": "12.0.2",
    "dotenv-flow": "4.1.0",
    "dotenv-flow-cli": "1.1.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.4.5",
    "npm-run-all2": "8.0.4",
    "pagefind": "1.3.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "sirv-cli": "3.0.1",
    "tailwindcss": "4.1.11",
    "tinyrainbow": "2.0.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "webpack": "5.100.2"
  }
}
